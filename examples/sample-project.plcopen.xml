      <pou name="MainProgram" pouType="program">
        <body>
          <ST><![CDATA[
PROGRAM MainProgram
  VAR
    Counter : INT := 0;
    MotorOn : BOOL := FALSE;
  END_VAR
  Counter := Counter + 1;
  MotorOn := (Counter MOD 2) = 0;
END_PROGRAM
]]></ST>
